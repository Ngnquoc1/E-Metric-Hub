# Server Configuration
PORT=5000
NODE_ENV=development

# Frontend URL (for CORS)
FRONTEND_URL=http://localhost:5173

# Shopee API Configuration (Real - for production)
SHOPEE_PARTNER_ID=your_partner_id_here
SHOPEE_PARTNER_KEY=your_partner_key_here
SHOPEE_REDIRECT_URL=http://localhost:5173/auth/shopee/callback

# Mock Mode (set to true for development without real Shopee account)
USE_MOCK_MODE=true

# Database Configuration (optional - for storing tokens)
# DB_HOST=localhost
# DB_PORT=5432
# DB_NAME=emetric_hub
# DB_USER=postgres
# DB_PASSWORD=your_password
